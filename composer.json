{
  "name": "ebitkov/todoist-php-sdk",
  "description": "Non-official PHP SDK of the Todoist REST API",
  "authors": [
    {
      "name": "Emanuel Bitkov",
      "email": "emanuel@bitkov.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "ebitkov\\TodoistSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ebitkov\\TodoistSDK\\Test\\": "test/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "doctrine/collections": "^1.6",
    "jms/serializer": "^3.17"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
